{% extends 'base.html.twig' %}

{% block body %}
    <h3>Mettre à jour une personne {{ form.vars.value.email }}</h3>

    {{ form_start(form) }}
        {{ form_row(form.debut) }}
        {{ form_row(form.fin) }}
        {{ form_row(form.type) }}
        <button class="btn btn-primary">Déposer</button>
    {{ form_end(form) }}
{% endblock %}
